@inherits Umbraco.Web.Macros.PartialViewMacroPage
@{

    string titleHeader = @Model.MacroParameters["TitleHeader"] != null ? @Model.MacroParameters["TitleHeader"].ToString() : "";
   string centerText = @Model.MacroParameters["CenterText"] != null ? @Model.MacroParameters["CenterText"].ToString() : "";



}
<body>
    <div class="header-image">
        <div class="img" style="background-image: url('@Umbraco.TypedMedia(Model.MacroParameters["HeaderImage"].ToString()).Url()');
             alt="Covid Resources">
            <div class="container">
                <div class="in">
                    <h1 id="texto"><strong> @titleHeader </strong></h1>
                </div>
                <h2>
                    <strong>@Html.Raw(centerText) </strong>
                </h2>
                <div class="inner">
                    <h3>
                        <span>
                            Discover the latest talent management and professional development trends with
                            <strong> The KNOWLEDGE<em>force</em>Resource <sup>(R)</sup></strong>
                            <br>
                            Explore our industry expertise below.
                        </span>
                    </h3>
                    <div class="button">
                        <a href="#" class="contact-link">Contact Us</a>
                    </div>
                </div>
            </div>
        </div>
    </div>
